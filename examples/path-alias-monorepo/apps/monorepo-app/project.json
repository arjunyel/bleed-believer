{
  "name": "monorepo-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/monorepo-app",
  "projectType": "application",
  "tags": [],
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/monorepo-app",
        "commands": [
          "node --no-warnings --loader ../../../../packages/path-alias/dist/loader/index.mjs ./src/main.mts"
        ]
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/monorepo-app",
        "commands": [
          "tsc",
          "tsc-alias",
          "node --no-warnings --loader ../../../../packages/path-alias/dist/loader/index.mjs ../../dist/apps/monorepo-app/src/main.mjs"
        ],
        "parallel": false
      }
    }
  }
}
